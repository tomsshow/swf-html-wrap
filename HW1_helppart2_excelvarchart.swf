<script type="text/javascript">
if ( document.getElementById( "mainContentPlaceholder_ViewInfoBar_scInfo" ) &&
     document.getElementById( "mainContentPlaceholder_ViewInfoBar_scInfoTimeout" ) )
{
   setTimeout( "stopStatusTimer()", document.getElementById( "mainContentPlaceholder_ViewInfoBar_scInfoTimeout" ).getAttribute( "value" ) );
}

function stopStatusTimer()
{   
   if ( document.getElementById( "mainContentPlaceholder_ViewInfoBar_scInfo" ) )
   {
      document.getElementById( "mainContentPlaceholder_ViewInfoBar_scInfo" ).style.display = "none";
   }
}

function displayMessage( controlId, message, append )
{
   if ( document.getElementById( controlId ) )
   {
      document.getElementById( controlId ).style.display = "block";
      
      if ( append && append == true )
      {
         document.getElementById( controlId ).innerHTML += "<p>" + message + "</p>";
      }
      else
      {
         document.getElementById( controlId ).innerHTML = "<p>" + message + "</p>";
      }
   }
}

function displayError( errorMsg, append )
{
   displayMessage( "mainContentPlaceholder_ViewInfoBar_scError", errorMsg, append );
}

function displayWarning( warningMsg, append )
{
   displayMessage( "mainContentPlaceholder_ViewInfoBar_scWarning", warningMsg, append );
}

function displayInfo( statusMsg, append, timeout )
{
   displayMessage( "mainContentPlaceholder_ViewInfoBar_scInfo", statusMsg, append );
   
   if ( document.getElementById( "mainContentPlaceholder_ViewInfoBar_scInfo" ) && timeout )
   {
      setTimeout( "stopStatusTimer()", timeout );
   }
}

function clearAllMessages()
{
   document.getElementById("mainContentPlaceholder_ViewInfoBar_scError").innerHTML = "";
   document.getElementById("mainContentPlaceholder_ViewInfoBar_scError").style.display = "none";
   document.getElementById("mainContentPlaceholder_ViewInfoBar_scWarning").innerHTML = "";
   document.getElementById("mainContentPlaceholder_ViewInfoBar_scWarning").style.display = "none";
   document.getElementById("mainContentPlaceholder_ViewInfoBar_scInfo").innerHTML = "";
   document.getElementById("mainContentPlaceholder_ViewInfoBar_scInfo").style.display = "none";
}
</script>

